        "@types/express": "^4.17.21",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^20.11.20",
        "concurrently": "^8.2.2",
        "prettier": "^3.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
